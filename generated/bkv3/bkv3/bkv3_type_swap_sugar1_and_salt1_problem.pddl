		(preference tried-belief-fresh-milk (tried_verify_belief_fresh_milk))
		(preference verified-fresh-milk (verified_fresh_milk milk2))
		))
	(:metric minimize (+
		(total-cost)
		(is-violated salt-verified)
		(is-violated raw-egg-verified)
		(is-violated beated-raw-egg1)
		(is-violated salted-raw-egg1)
		(is-violated not-sugared-raw-egg1)
		(is-violated cooked-raw-egg1)
		(is-violated plated-raw-egg1)
