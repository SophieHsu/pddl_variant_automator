		(is_raw_egg raw_egg1)
		(is_boiled_egg boiled_egg1)
		(is_sugar sugar1)
		(is_salt salt1)
		(is_spoiled_milk milk1)
		(is_fresh_milk milk2)
		(is_cooking)
